<!-- The properties 'jar', 'outfile', 'fileVersion', 'txtFileVersion', 
     'productVersion' and 'txtProductVersion' are overruled by the 
     installer build.xml file -->

<launch4jConfig>
  <dontWrapJar>false</dontWrapJar>
  <headerType>gui</headerType>
  <jar>../geonetwork-2.6.0/geonetwork-install.jar</jar>
  <outfile>../geonetwork-2.6.0/geonetwork-install.exe</outfile>
  <errTitle>GeoNetwork opensource Installation Error</errTitle>
  <stayAlive>false</stayAlive>
  <icon>../installer.ico</icon>
  <jre>
    <minVersion>1.6.0</minVersion>
    <initialHeapSize>256</initialHeapSize>
    <maxHeapSize>512</maxHeapSize>
  </jre>
  <splash>
    <file>../splash.bmp</file>
    <waitForWindow>true</waitForWindow>
    <timeout>60</timeout>
    <timeoutErr>true</timeoutErr>
  </splash>
  <versionInfo>
    <fileVersion>2.6.0.0</fileVersion>
    <txtFileVersion>2.6.0 0</txtFileVersion>
    <fileDescription>GeoNetwork opensource Windows Installer</fileDescription>
    <copyright>OSGeo (http://www.osgeo.org)</copyright>
    <productVersion>2.6.0.0</productVersion>
    <txtProductVersion>2.6.0 0</txtProductVersion>
    <productName>GeoNetwork opensource</productName>
    <companyName></companyName>
    <internalName>geonetwork-install.jar</internalName>
    <originalFilename>geonetwork-install.exe</originalFilename>
  </versionInfo>
</launch4jConfig>
